@font-face {
    font-family: {FONT_NAME};
    src: url('./fonts/museo_sans/{FONT_FILE}.eot');
    src: url('./fonts/museo_sans/{FONT_FILE}.eot?#iefix') format('embedded-opentype'),
         url('./fonts/museo_sans/{FONT_FILE}.woff2') format('woff2'),
         url('./fonts/museo_sans/{FONT_FILE}.woff') format('woff'),
         url('./fonts/museo_sans/{FONT_FILE}.ttf') format('truetype');
    font-weight: {FONT_WEIGHT};
    font-style: {FONT_STYLE};
}
